<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.skylake.mov">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.CAMERA"/>
    
    <uses-permission-sdk-23 android:name="android.permission.INTERNET"/>
    <uses-permission-sdk-23 android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission-sdk-23 android:name="android.permission.CAMERA"/>

    <uses-feature android:name="andorid.hardware.camera"
        android:required="true"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.noActionBar">
        <activity android:name=".DetailActivity"/>
        <activity android:name=".HomeActivity" />
        <activity android:name=".onboarding.OnboardingOneActivity" />
        <activity android:name=".onboarding.OnboardingTwoActivity" />
        <activity android:name=".onboarding.OnboardingThreeActivity" />
        <activity android:name=".signin.signin.SignInActivity" />
        <activity android:name=".signin.signup.SignUpActivity" />
        <activity android:name=".signin.signup.SignUpPhotoScreenActivity" />
        <activity android:name=".PilihBangkuActivity" />
        <activity android:name=".CheckoutActivity" />
        <activity android:name=".EditProfileActivity" />
        <activity android:name=".MyWalletActivity" />
        <activity android:name=".SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>